{
  "name":             "itee-plugin-three",
  "version":          "1.7.0",
  "description":      "This itee plugin allow to use three js content from end to end in an itee client-server-database architecture",
  "keywords":         [
    "itee",
    "plugin",
    "three",
    "threejs"
  ],
  "author":           {
    "name": "Itee (Tristan Valcke)",
    "url":  "https://github.com/Itee"
  },
  "contributors":     [],
  "license":          "BSD-3-Clause",
  "private":          false,
  "repository":       {
    "type": "git",
    "url":  "https://github.com/Itee/itee-plugin-three.git"
  },
  "type":             "module",
  "main":             "builds/itee-plugin-three.cjs.js",
  "module":           "builds/itee-plugin-three.esm.js",
  "browser":          "builds/itee-plugin-three.iife.js",
  "homepage":         "https://github.com/Itee/itee-plugin-three#readme",
  "man":              "./docs/index.html",
  "bugs":             {
    "url": "https://github.com/Itee/itee-plugin-three/issues"
  },
  "engines":          {
    "node": ">=20"
  },
  "os":               [
    "win32",
    "linux"
  ],
  "cpu":              [
    "x64",
    "ia32"
  ],
  "scripts":          {
    "default":                     "gulp --tasks",
    "refresh":                     "node ./.tasks/_refresh.mjs",
    "help":                        "gulp help",
    "patch":                       "gulp patch",
    "clean":                       "gulp clean",
    "lint":                        "gulp lint",
    "doc":                         "gulp doc",
    "build":                       "gulp build",
    "tests:run":                   "gulp run-tests",
    "tests:bundling":              "gulp check-bundling",
    "tests:bundling:build-import": "gulp check-bundling-from-esm-build-import",
    "tests:bundling:files-import": "gulp check-bundling-from-esm-files-import",
    "tests:bundling:files-direct": "gulp check-bundling-from-esm-files-direct",
    "tests:benches:compute":       "gulp compute-benchmarks",
    "tests:benches:run":           "gulp run-benchmarks",
    "tests:benches:run:back":      "gulp run-benchmarks-for-backend",
    "tests:benches:run:front":     "gulp run-benchmarks-for-frontend",
    "tests:units:compute":         "gulp compute-unit-tests",
    "tests:units:run":             "gulp run-unit-tests",
    "tests:units:run:back":        "gulp run-unit-tests-for-backend",
    "tests:units:run:front":       "gulp run-unit-tests-for-frontend",
    "release":                     "gulp release",
    "semantic-release":            "semantic-release --dry-run --no-ci",
    "postversion":                 "gulp build"
  },
  "dependencies":     {
    "bson":            "^6.10.4",
    "itee-client":     "^8.3.0",
    "itee-core":       "^1.4.0",
    "itee-database":   "^8.3.0",
    "itee-mongodb":    "^1.2.0",
    "itee-utils":      "^5.6.0",
    "itee-validators": "^5.6.0"
  },
  "peerDependencies": {
    "three":      "^0.149.0",
    "three-full": "^28.0.2"
  },
  "devDependencies":  {
    "@eslint/js":                  "^9.38.0",
    "@rollup/plugin-alias":        "^5.1.1",
    "@rollup/plugin-commonjs":     "^28.0.9",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser":       "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git":       "^10.0.1",
    "@semantic-release/github":    "^11.0.6",
    "@semantic-release/npm":       "^12.0.2",
    "@web/test-runner":            "^0.20.2",
    "@web/test-runner-playwright": "^0.11.1",
    "ansi-colors":                 "^4.1.1",
    "benchmark":                   "^2.1.4",
    "chai":                        "^6.2.0",
    "cz-emoji":                    "^1.3.2-canary.2",
    "del":                         "^8.0.1",
    "eslint":                      "^9.38.0",
    "eslint-plugin-mocha":         "^11.2.0",
    "fancy-log":                   "^2.0.0",
    "figlet":                      "^1.9.3",
    "glob":                        "^11.0.3",
    "globals":                     "^16.4.0",
    "gulp":                        "^5.0.1",
    "ink-docstrap":                "^1.3.2",
    "jsdoc":                       "^4.0.5",
    "mocha":                       "^11.7.4",
    "mochawesome":                 "^7.1.4",
    "rollup":                      "^4.52.5",
    "rollup-plugin-cleanup":       "^3.2.1",
    "rollup-plugin-re":            "^1.0.7",
    "semantic-release":            "^24.2.9",
    "semantic-release-gitmoji":    "^1.6.8",
    "taffydb":                     "^2.7.3"
  }
}
